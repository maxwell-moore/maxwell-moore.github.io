{  
  DropDownMenu.Enable(id, icon, text, tooltip, DropDownMenu.g_GridStyles);
}
GridDropDownMenu.Disable = function(id, icon, text, tooltip)
{  
  DropDownMenu.Disable(id, icon, text, tooltip, DropDownMenu.g_GridStyles);
}


